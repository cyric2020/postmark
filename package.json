{
  "name": "postmark-md-renderer",
  "version": "1.1.0",
  "description": "A modified version of Simple-Markdown that can render Markdown to HTML with KaTeX and Code support.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify --require postmark --outfile file.js",
    "convert": "npx babel index.mjs --out-file index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "github.com/Cyric2020/postmark"
  },
  "author": "Cyric2020",
  "license": "ISC",
  "dependencies": {
    "@khanacademy/simple-markdown": "^2.0.0",
    "highlight.js": "^11.6.0",
    "katex": "^0.16.3",
    "open": "^10.0.1"
  },
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7"
  }
}
